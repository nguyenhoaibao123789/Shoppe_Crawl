{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "ModHeader - Modify HTTP headers",
  "version": "5.0.5",
  "description": "Modify HTTP request headers, response headers, and redirect URLs",
  "short_name": "ModHeader",
  "homepage_url": "https://modheader.com/",
  "background": {
    "scripts": [
      "assets/background-ffc24c3e.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "assets/common-4aa3a322.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "assets/theCard-746d34e6.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "ModHeader",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "incognito": "spanning",
  "permissions": [
    "alarms",
    "contextMenus",
    "webRequest",
    "storage",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": [
      "*://modheader.com/*"
    ]
  },
  "storage": {
    "managed_schema": "schema.json"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H"
      }
    },
    "toggle_pause": {
      "description": "Toggle pause/resume",
      "suggested_key": {
        "default": "Alt+Shift+P"
      }
    },
    "switch_to_profile_1": {
      "description": "Switch to 1st profile"
    },
    "switch_to_profile_2": {
      "description": "Switch to 2nd profile"
    },
    "switch_to_profile_3": {
      "description": "Switch to 3rd profile"
    },
    "switch_to_profile_4": {
      "description": "Switch to 4th profile"
    }
  },
  "web_accessible_resources": [
    "profile-hook-58c2ff11.js",
    "commonjsHelpers-c9e3b764.js",
    "App-4876f061.js",
    "js/tool/common.js",
    "js/tool/theCard.js"
  ]
}